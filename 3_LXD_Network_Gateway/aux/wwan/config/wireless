
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'pci0000:00/0000:00:1c.0/0000:02:00.0'
	option htmode 'VHT80'
	option disabled '0'
	option country 'US'
	option legacy_rates '1'
	option channel 'auto'

config wifi-iface
	option network 'wwan'
	option ssid 'homenet'
	option device 'radio0'
	option mode 'sta'
	option bssid 'F0:9F:C2:F4:74:3B'
	option key 'ilove124'
	option encryption 'psk2+ccmp'
